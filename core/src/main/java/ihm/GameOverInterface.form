<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="ihm.GameOverInterface">
  <grid id="mainPanel" binding="mainPanel" layout-manager="GridLayoutManager" row-count="4" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="20" left="20" bottom="20" right="20"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <background color="-14678721"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="scoreLabel" class="javax.swing.JLabel" binding="scoreLabel">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="OCR A Extended" size="22" style="0"/>
          <foreground color="-1"/>
          <text value="Score: 0"/>
        </properties>
      </component>
      <component id="highScoreLabel" class="javax.swing.JLabel" binding="highScoreLabel">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="OCR A Extended" size="20" style="2"/>
          <foreground color="-1"/>
          <text value="Meilleur score: 0"/>
        </properties>
      </component>
      <grid id="buttonsPanel" binding="buttonsPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="20" vgap="-1">
        <margin top="30" left="0" bottom="0" right="0"/>
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <background color="-14678721"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="restartButton" class="javax.swing.JButton" binding="restartButton">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font name="Arial" size="16" style="1"/>
              <foreground color="-1"/>
              <text value="Rejouer"/>
            </properties>
          </component>
          <component id="quitButton" class="javax.swing.JButton" binding="quitButton">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font name="Arial" size="16" style="1"/>
              <foreground color="-1"/>
              <text value="Quitter"/>
            </properties>
          </component>
        </children>
      </grid>
      <component id="gameOverLabel" class="javax.swing.JLabel" binding="gameOverLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Chiller" size="72" style="1"/>
          <foreground color="-11074287"/>
          <text value="GAME OVER"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
